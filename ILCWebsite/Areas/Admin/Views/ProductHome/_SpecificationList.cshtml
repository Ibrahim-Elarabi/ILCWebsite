@model List<ILC.BL.Models.Admin.HomeSection.Product.ProductSpecificationVM>
<div class="card">
    <div class="card-header">
        This Properties Saved in DB You Can Delete / Edit Any Property
    </div>
    <div class="card-body">
        <table @*id="data-table"*@ class="table  table-bordered  table-sm mt-3">
            <thead>
                <tr>
                    <th>Property Name EN</th>
                    <th>Property Name Ar</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody @*id="table-body"*@>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.TitleEn</td>
                        <td>@item.TitleAr</td>
                        <td>
                            <button class="btn btn-danger btn-sm" >Delete</button>
                        </td>
                    </tr>
                }
                <!-- Data will be displayed here -->
            </tbody>
        </table>
    </div>
</div>
