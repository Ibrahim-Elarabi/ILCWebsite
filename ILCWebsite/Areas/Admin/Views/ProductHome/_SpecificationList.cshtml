@model List<ILC.BL.Models.Admin.HomeSection.Product.ProductSpecificationVM>
<div class="card">
    <div class="card-header">
        This Properties Alreadr Exist You Can Delete Any Property
    </div>
    <div class="card-body">
        <table @*id="data-table"*@ class="table  table-bordered  table-sm mt-3">
            <thead>
                <tr>
                    <th>Name English</th>
                    <th>Name Arabic</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody @*id="table-body"*@>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.TitleEn</td>
                        <td>@item.TitleAr</td>
                        <td>
                            <a class="btn btn-danger btn-sm" onclick="Delete(@item?.ID,'/Admin/ProductHome/DeleteProp')">Delete</a>
                        </td>
                    </tr>
                }
                <!-- Data will be displayed here -->
            </tbody>
        </table>
    </div>
</div>
