@model List<ILC.BL.Models.Admin.HomeSection.Support.SupportHomeVM>
@using Microsoft.Extensions.Localization
@using ILCWebsite.Models.Resources
@{
    ViewData["Title"] = "List";
}
<a asp-controller="SupportHome" asp-action="Create">Create</a>
<div>
    @foreach (var item in Model)
    {
        <div style="background-color:#ddd">  
            <p>TitleAr : @item?.TitleAr</p>
            <p>TitleEn : @item?.TitleEn</p>
            <p>DescriptionAr : @item?.DescriptionAr</p>
            <p>DescriptionEn : @item?.DescriptionEn</p>
            <p>LocationAr : @item?.LocationAr</p>
            <p>LocationEn : @item?.LocationEn</p>
            <p>Email : @item?.Email</p>
            <p>Phone : @item?.Phone</p>
            <a href="/Admin/SupportHome/Edit/@item?.Id"> Edit</a>
            <a href="/Admin/SupportHome/Details/@item?.Id"> Details</a>
            <button onclick="Delete(@item?.Id,'/Admin/SupportHome/Delete')"> Delete </button>
        </div> 
        <br />
    } 
</div>
@section Scripts{
    <script src="~/js/npm_axios_dist_axios.min.js"></script> 
    <script src="~/customscript/sahreddelete.js"></script>
}

